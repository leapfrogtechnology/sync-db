#!/usr/bin/env node

const fs = require('fs')
const path = require('path')
const project = path.join(__dirname, '../lib/')
const prod = fs.existsSync(project)

if (!prod) {
  require('ts-node').register({project})
}

require(`../${prod ? 'lib' : 'src'}`).run()
.catch(require('@oclif/errors/handle'))
