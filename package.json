  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/lib"
  ],
  "bin": {
    "sync-db": "./bin/run"
  },
  "oclif": {
    "bin": "sync-db"
  },
  "repository": "https://github.com/leapfrogtechnology/sync-db",
  "homepage": "https://github.com/leapfrogtechnology/sync-db",
  "bugs": "https://github.com/leapfrogtechnology/sync-db/issues",
  "keywords": [
    "sync-db",
    "dbo",
